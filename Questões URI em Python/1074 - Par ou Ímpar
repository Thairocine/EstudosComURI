numero = int(input())

for i in range(numero):
    numero_novos = int(input())
    
    if (numero_novos > 0):
        if(numero_novos % 2 == 0):
            print("EVEN POSITIVE")
        else:
            print("ODD POSITIVE")
        
    elif (numero_novos < 0):
        if(numero_novos % 2 == 0):
            print("EVEN NEGATIVE")
        else:
            print("ODD NEGATIVE")
    else: 
        print("NULL")
