result = 0

entrada = str(input()).split()

codigo = int(entrada[0])
quantidade = float(entrada[1])

if codigo == 1:
    result = quantidade * 4

elif codigo == 2:
    result = quantidade * 4.50

elif codigo == 3:
    result = quantidade * 5

elif codigo == 4:
    result = quantidade * 2

elif codigo == 5:
    result = quantidade * 1.50

print(f'Total: R$ {result:.2f}')
